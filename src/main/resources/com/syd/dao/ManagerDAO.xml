<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.syd.dao.ManagerDAO">
    <sql id="table">manager</sql>
    <sql id="selectFields">id, name, password, salt, head_url, gender, iphone, email, date, status
    </sql>
    <select id="getManagerList_Page" resultType="com.syd.model.Manager">
        SELECT
        <include refid="selectFields"/>
        FROM
        <include refid="table"/>
        ORDER BY id
        LIMIT #{offset},#{limit}
    </select>
</mapper>
